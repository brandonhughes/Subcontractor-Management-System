{
  "name": "subcontractor-management-system",
  "version": "1.0.0",
  "description": "Subcontractor Management System - A platform for managing and reviewing subcontractors",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm install",
    "build:all": "npm run build:frontend && npm run build:backend",
    "build:render": "node render-build.js && cd render-build && npm install && npm run build:all",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "http": "^0.0.1-security"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhughes/Subcontractor-Management-System.git"
  },
  "keywords": [
    "subcontractor",
    "management",
    "review",
    "scoring",
    "fullstack"
  ],
  "author": "Brandon Hughes",
  "license": "MIT"
}